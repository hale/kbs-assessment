                <hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
                  >Fly Fishing</hasTitle>
              </Book>
            </hasBook>
          </Purchase>
        </hasMadePurchase>
      </Customer>
    </hasBuyer>
  </Purchase>

  <Purchase rdf:ID="Purchase_5">
    <hasBook rdf:resource="#Moby_Dick"/>
